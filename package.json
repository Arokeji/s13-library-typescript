{
  "name": "simple-template",
  "version": "1.0.0",
  "description": "Hola este es el repo de template",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev ./src/index.ts",
    "start:pro": "npm run build && node ./dist/index.js",
    "seed:books": "nodemon ./seeds/book.seed.js",
    "seed:authors": "nodemon ./seeds/author.seed.js",
    "seed:relations": "nodemon ./seeds/book-relations.seed.js",
    "seed:all": "npm run seed:authors && npm run seed:books && npm run seed:relations",
    "normalize:authors": "nodemon ./normalization/author.normalization.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.5",
    "multer": "^1.4.5-lts.1",
    "validator": "^13.9.0"
  }
}
